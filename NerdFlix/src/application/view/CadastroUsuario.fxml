<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>

<AnchorPane prefHeight="450.0" prefWidth="300.0" styleClass="background" stylesheets="@../css/style.css" xmlns="http://javafx.com/javafx/8.0.101" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.view.CadastroUsuarioController">
   <children>
      <Label layoutX="25.0" layoutY="34.0" text="Nome:">
         <padding>
            <Insets bottom="50.0" />
         </padding>
      </Label>
      <Label layoutX="27.0" layoutY="176.0" text="Login:">
         <padding>
            <Insets bottom="50.0" />
         </padding>
      </Label>
      <Label layoutX="28.0" layoutY="105.0" text="Email:">
         <padding>
            <Insets bottom="50.0" />
         </padding>
      </Label>
      <Label layoutX="26.0" layoutY="247.0" text="Senha:">
         <padding>
            <Insets bottom="50.0" />
         </padding>
      </Label>
      <Label layoutX="28.0" layoutY="318.0" text="Idade:">
         <padding>
            <Insets bottom="50.0" />
         </padding>
      </Label>
      <HBox layoutX="28.0" layoutY="389.0" spacing="30.0">
         <children>
            <Button mnemonicParsing="false" onAction="#handleCadastrarButton" text="Cadastrar" />
            <Button mnemonicParsing="false" onAction="#handleCancelarButton" text="Cancelar" />
         </children>
      </HBox>
      <TextField fx:id="nomeTextF" layoutX="99.0" layoutY="34.0" />
      <TextField fx:id="emailTextF" layoutX="99.0" layoutY="105.0" />
      <TextField fx:id="loginTextF" layoutX="99.0" layoutY="176.0" />
      <TextField fx:id="senhaTextF" layoutX="99.0" layoutY="247.0" />
      <TextField fx:id="idadeTextF" layoutX="99.0" layoutY="318.0" />
   </children>
</AnchorPane>
